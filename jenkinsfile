node ('GOLBUILD'){
	stage ('GOL REPO'){
		git credentialsId: 'b1f5f594-0e99-4c89-b613-08b7e58e2923', url: 'https://github.com/Challanges/game-of-life.git'
	}

	stage ('GOLPKG-BILD'){
		sh label: '', script: 'mvn clean  test package'	
	}
	
	
	stage ('List Artifacts Upload'){
		archiveArtifacts 'gameoflife-web/target/*.jar,gameoflife-web/target/*.war'
        	##### sh label: '', script: '''sudo aws s3 cp $HOME/workspace/golbould1/gameoflife-web/target/*.war s3://1mynew-bucket/'''
	}

	
}

node ('GOLDEV'){
	stage ('GOL REPO'){
		git credentialsId: 'b1f5f594-0e99-4c89-b613-08b7e58e2923', url: 'https://github.com/Challanges/game-of-life.git'
	}

	stage ('GOL INSTALL'){
		sh label: '', script: 'mvn install'	
	
	}

	stage ('GOL Run'){
		sh label: '', script: 'mvn jetty:run'	
		}
}

